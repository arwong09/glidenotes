<form id="new-submit" action="#" method="get" accept-charset="utf-8">
  <input type="hidden" name="card[list_id]" value="<%= list.get('id') %>">
  <input type="hidden" name="card[rank]" value="0">
  
  <div class="form-group">
    <input class='form-control' type="text" 
    name="card[title]" id="form_title">
  </div>

  <div class="form-group">
    <input type="submit" id="input_title" 
    class='btn btn-primary' value="create">
  </div>
</form>



